  "files": [
    "index.js"
  ],
  "dependencies": {
    "nlcst-to-string": "^2.0.0",
    "repeat-string": "^1.5.2",
    "vfile-location": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^13.0.1",
    "esmangle": "^1.0.1",
    "is-hidden": "^1.0.1",
    "negate": "^1.0.0",
    "nyc": "^8.3.0",
    "parse-dutch": "^3.0.0",
    "parse-english": "^3.0.0",
    "parse-latin": "^3.1.1",
