{
  "name": "retext-simplify",
  "version": "3.0.0",
  "description": "Check phrases for simpler alternatives",
  "license": "MIT",
  "keywords": [
    "simple",
    "simpler",
    "synonym",
    "alternative",
    "phrase",
    "retext",
    "plugin"
  ],
