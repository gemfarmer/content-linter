{
  "name": "nspell",
  "version": "1.0.2",
  "description": "Hunspell compatible spell checker",
  "license": "MIT",
  "keywords": [
    "spell",
    "spellcheck",
    "spellchecker",
    "spell check",
    "spell checker",
    "check",
    "spelling",
    "myspell",
    "hunspell"
  ],
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "dependencies": {
    "has": "^1.0.1",
    "is-buffer": "^1.1.4",
    "trim": "0.0.1",
    "x-is-string": "^0.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooorm/nspell.git"
  },
  "bugs": {
    "url": "https://github.com/wooorm/nspell/issues"
  },
  "author": {
    "name": "Titus Wormer",
    "email": "tituswormer@gmail.com",
    "url": "http://wooorm.com"
  },
  "contributors": [
    {
      "name": "Titus Wormer",
      "email": "tituswormer@gmail.com",
      "url": "http://wooorm.com"
    }
  ],
  "devDependencies": {
    "bail": "^1.0.1",
    "browserify": "^13.0.0",
    "dictionary-da-dk": "^1.0.2",
    "dictionary-de": "^1.0.2",
    "dictionary-en-gb": "^1.0.2",
    "dictionary-en-us": "^1.0.2",
    "dictionary-nl": "^1.0.2",
    "esmangle": "^1.0.0",
    "levenshtein-edit-distance": "^2.0.0",
    "nyc": "^10.0.0",
    "remark-cli": "^2.1.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.2.0",
    "time-span": "^1.0.0",
    "xo": "^0.17.1"
  },
  "scripts": {
    "build-md": "remark . -qfo",
    "build-bundle": "browserify lib/index.js --bare -s nspell > nspell.js",
    "build-mangle": "esmangle nspell.js > nspell.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run build && npm run lint && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "xo": {
    "space": true,
    "rules": {
      "guard-for-in": "off",
      "max-depth": "off",
      "complexity": "off"
    },
    "ignores": [
      "nspell.js",
      "nspell.min.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  },
