{
  "name": "detab",
  "version": "2.0.0",
  "description": "Detab: tabs -> spaces",
  "license": "MIT",
  "keywords": [
    "detab",
    "tab",
    "space",
    "tab-size",
    "size"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "repeat-string": "^1.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wooorm/detab.git"
  },
  "bugs": {
    "url": "https://github.com/wooorm/detab/issues"
  },
  "author": {
    "name": "Titus Wormer",
